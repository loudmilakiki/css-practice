<html>

  <head>
    <title>
      dashboard page
    </title>
    <meta charset='utf-8' />

    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/site.webmanifest' />
    <link
      rel='mask-icon'
      href='/fav/safari-pinned-tab.svg'
      color='#5bbad5'
    />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />

  </head>

  <body>
    <div class='page page--dashboard'>
      <aside
        class='page__aside aside vertical vertical--big'
      >
        <div class='text aside__logo'>
          Sidebar
        </div>
        <nav class='vertical vertical--small'>
          <a
            href='#'
            class='nav-button nav-button--active click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/home.svg')"
            ></span>
            <span class='text text--light'>Home</span>
          </a>

          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/orders.svg')"
            ></span>
            <span class='text text--light'>Orders</span>
          </a>

          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/products.svg')"
            ></span>
            <span class='text text--light'>Products</span>
          </a>

        </nav>
      </aside>

      <header class='header'>
        <div class='layout header__content text--small'>
          <div class='header__search relative'>
            <input
              type='text'
              placeholder='Search'
              class='text field field--header'
            />
            <span
              class='icon header__icon-search'
              style="background-image: url('/svg/header/search.svg')"
            ></span>
            <span
              class='icon icon--header-settings icon--button icon--color-blue click'
              style="background-image: url('/svg/header/settings.svg')"
            ></span>
          </div>

          <div class='header__list relative'>

            <span
              class='icon icon--header-signal icon--button icon--color-signal click'
              style="background-image: url('/svg/header/signal.svg')"
            ></span>

            <span
              class='icon icon--header-translate icon--button icon--color-translate click'
              style="background-image: url('/svg/header/translate.svg')"
            ></span>

            <span
              class='icon icon--header-bell icon--button icon--color-signal click'
              style="background-image: url('/svg/header/bell.svg')"
            ></span>
          </div>
        </div>
      </header>

      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block'>
            <div
              class='analytic analytic--double analytic--blue'
            >

              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/order.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$961</div>
                <div class='text--small text--lighter'>Total
                  Order</div>
                <span
                  class='icon analytic__arrow'
                  style="background-image: url('/svg/analytic/blue-arrow.svg')"
                ></span>
              </div>
            </div>

            <div
              class='analytic analytic--double analitic--purple'
            >

              <span
                class='icon analytic__button icon--button icon--color-purple click'
                style="background-image: url('/svg/analytic/money.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$500.00</div>
                <div class='text--small text--lighter'>
                  Total earning</div>
                <span
                  class='icon analytic__money'
                  style="background-image: url('/svg/analytic/purple-arrow.svg')"
                ></span>
              </div>

            </div>

            <div class='analytic-half analytic--blue'>

              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/money.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text-half text--small'
                >$203k</div>
                <div class='text--small text--lighter'>
                  Total Income</div>
              </div>

            </div>

            <div class='analytic-half analitic--white'>

              <span
                class='icon analytic__button icon--button icon--color-shop click'
                style="background-image: url('/svg/analytic/shop.svg')"
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--shop'
                >$203k</div>
                <div class='text--gray text--shop'>
                  Total Income</div>

              </div>

            </div>
          </section>

          <section class='chackout'>
            <div class='purchase'>
              <div class='text text--shop'>Purchase list
              </div>
              <div class='line'></div>

              <div class='purchase__list'>
                <div class='info'>
                  ID437661
                  <p>May 11, 2023, 14:21</p>
                </div>
                <div class='info'>AirOn ProCam
                  <p>7/8 USB Type-C</p>
                </div>
                <div class='info'>
                  132.32 EUR
                  <p class='text--warning'>Not paid</p>
                </div>
                <div class='info'>
                  Customer
                  <p>robin@com.de</p>
                </div>
              </div>

              <div class='line'></div>

              <div class='purchase__list'>
                <div class='info'>
                  ID437661
                  <p>May 11, 2023, 14:21</p>
                </div>
                <div class='info'>PHILIPS Essential
                  <p>HD9200/90</p>
                </div>
                <div class='info'>
                  543 USD
                  <p class='text--seccess'>Pay by Stripe</p>
                </div>
                <div class='info'>
                  Customer
                  <p>dima@com.ua</p>
                </div>
              </div>

            </div>

            <div class='payment'>

              <div class='text text--shop'>Payment
              </div>

              <div class='line'></div>

              <div class='form'>
                <input
                  class='form-check'
                  type='radio'
                />Credit card</div>

              <div class='form'>
                <input
                  class='form-check'
                  type='radio'
                />Debit card</div>

              <div class='form'>
                <input
                  class='form-check'
                  type='radio'
                />PayPal</div>

              <div class='cart1'>

                <div>Name on card
                  <input
                    type='text'
                    class='text field'
                  /></div>

                <div>Credit card number
                  <input
                    type='text'
                    class='text field'
                  /></div>
              </div>

              <div class='cart2'>
                <div>Expiration

                  <input
                    type='text'
                    class='text field'
                  /></div>

                <div>CVV

                  <input
                    type='text'
                    class='text field'
                  /></div>
              </div>

              <div class='line'></div>

              <div class='choose-block'>

                <div class='text--gray'>Country
                  <div class='relative click'>
                    <div class='field field--select'>
                      <span
                        class='text text--gray'
                      >Choose...</span>
                      <div>
                        <select class='field__select'>
                          <option value='dog'>Dog</option>
                          <option value='cat'>Cat</option>
                          <option value='fish'>Fish</option>
                          <option
                            value='hamster'
                          >Hamster</option>
                        </select>
                      </div>
                    </div>
                  </div>
                </div>

                <div>State
                  <div class='relative click'>
                    <div class='field field--select'>

                      <span
                        class='text text--gray'
                      >Choose...</span>
                      <div>
                        <select class='field__select'>
                          <option
                            value='ukraine'
                          >Ukraine</option>
                          <option
                            value='qatar'
                          >Qatar</option>
                          <option
                            value='lebanon'
                          >Lebanon</option>

                        </select>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class='line'></div>

              <div class='check'>
                <nav class='vertical vertical--small'>
                  <a
                    href='#'
                    class='nav-buttons nav-button--active click'
                  >

                    <span class='text text--check'>Continue
                      to checkout</span>
                  </a>
                </nav>
              </div>

            </div>

          </section>
        </div>
      </main>
    </div>
  </body>

</html>